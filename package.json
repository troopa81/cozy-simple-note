{
	"name": "simple-note",
	"version": "0.1.3",
	"description": "Simple way for taking note note Cozy application",
	"main": "server.js",
	"dependencies": {
		"americano": "0.4.2",
		"body-parser": "1.14.2",
		"cozydb": "0.1.7",
		"express": "4.13.3",
		"morgan": "1.6.1"
	},
	"devDependencies": {
		"nodemon": "1.8.1"
	},
	"scripts": {
		"start": "node server.js",
		"dev": "node_modules/.bin/nodemon server.js --ignore client/"
	},
	"cozy-permissions": {
		"Note": {
			"description": "Creates and edits your notes."
		}
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/troopa81/cozy-simple-note"
	},
	"keywords": [
		"cozy",
		"simple",
		"note"
	],
	"author": "Julien Cabieces",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/troopa81/cozy-simple-note/issues"
	},
	"homepage": "https://github.com/troopa81/cozy-simple-note"
}
